<div class="container">
  <div class="well">
    <%= link_to "New Idea", new_idea_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-idea"></div>
  
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Quality</th>
        <td>&nbsp;</td><td>&nbsp;</td>
      </tr>
    </thead>
    <tbody class="idea-index">
      <%= render "index" %>
    </tbody>
  </table>
</div>
<div id="idea-modal" class="modal fade"></div>